   ],
   "source": [
    "# 일반식\n",
    "def is_even(n):\n",
    "    if n % 2 ==0:\n",
    "        print(True)\n",
    "    else:\n",
    "        n % 2 !=0\n",
    "        print(False)\n",
    "\n",
    "is_even(3)\n",
    "\n",
    "# 조기 리턴식\n",
    "def is_even2(x):\n",
    "    return x % 2 ==0\n",
    "\n",
    "is_even2(4)"
   ]
  }
