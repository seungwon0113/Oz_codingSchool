   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "최소값: 4 최대값: 10\n"
     ]
    }
   ],
   "source": [
    "#참고용 코드입니다.\n",
    "def min_max(*numbers):\n",
    "    numbers_tuple = tuple(numbers)\n",
    "    print(\"최소값:\", min(numbers_tuple), \"최대값:\", max(numbers_tuple))\n",
    "\n",
    "# 사용 예시\n",
    "min_max(10, 4, 9)"
   ]
  }
